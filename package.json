{
  "name": "core-async-boomerang",
  "version": "1.0.0",
  "description": "Настало время разрабатывать лучшие в мире игры!",
  "main": "run.js",
  "dependencies": {
    "find-exec": "^1.0.2",
    "keypress": "^0.2.1",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "play-sound": "^1.1.5",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "globals": "^15.12.0"
  },
  "scripts": {
    "game": "node run.js Lisarus",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dbCreate": "npx sequelize db:create",
    "dbMigrate": "npx sequelize db:migrate",
    "dbSeedAll": "npx sequelize db:seed:all",
    "dbDrop": "npx sequelize db:drop",
    "dbRemigrate": "npx sequelize db:migrate:undo:all && npx sequelize db:migrate && npx sequelize db:seed:all",
    "db:model:generate": "npx sequelize-cli model:generate --name Author --attributes firstName:string,age:integer",
    "db:seed:generate": "npx sequelize-cli seed:generate --name Computer"
  },
  "repository": {
